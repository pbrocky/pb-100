<script language="JavaScript">
	var _ActivexEnabled = _ActivexEnabled || ( function(){
		var tester;
		try {
			tester = new ActiveXObject('htmlfile');
		}
		catch (e) {
			// catch the exception
		}
		if (tester) {
			// ActiveX is installed
			return 'pbDisplay-ActiveX-enabled';
		}
		return '';
	})();
	
	if( _ActivexEnabled !== '' && !this.className.match( _ActivexEnabled)){
		this.className += ( this.className.length === 0 ? '' : ' ') +_ActivexEnabled;
	}
</script>

